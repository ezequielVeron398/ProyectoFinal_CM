class BalaVomito extends Proyectil {
  private Utilidad utilidad;
  private float diametro = 10;
  private int colliderVomito;
  private float da単o;

  public BalaVomito(PVector objetivo, PVector posicion) {
    this.posicion = posicion;
    this.utilidad = new Utilidad();
    this.direccion = this.utilidad.trayectoria(this.posicion, objetivo);
    this.colliderVomito = 5;
    this.da単o = 10;
  }
  public void mover() {
    this.posicion.x += this.direccion.x*this.velocidad*deltaTime;
    this.posicion.y += this.direccion.y*this.velocidad*deltaTime;
  }

  public void display() {
    fill(0, 255, 0);
    noStroke();
    ellipse(this.posicion.x, this.posicion.y, diametro, diametro);
  }
  public float getDa単o(){
    return da単o; 
  
  }
}
