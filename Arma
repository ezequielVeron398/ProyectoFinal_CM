 private PImage sprite;
  private int posicionFrameX;
  private int posicionFrameY;
  private int altoFrame;
  private int anchoFrame;
  private int contFrame;
  private int cantFrame;
  
 void setup(){
 sprite = requestImage("shoot2.png");
  posicionFrameX = 0;
  posicionFrameY = 0;
  altoFrame = 27;
  anchoFrame = 30;
  contFrame = 1;
  cantFrame = 5;
  frameRate(12);
 }
    


  void draw(){
    background(255);
    
    imageMode(CENTER);
    if(contFrame <= cantFrame){
    image(sprite.get(this.posicionFrameX,this.posicionFrameY,this.anchoFrame,this.altoFrame),width/2,height/2);
    if(this.posicionFrameY < sprite.height-this.altoFrame){
      posicionFrameY+=this.altoFrame;
    }else{
      posicionFrameY=0;
    }
    }
  }
